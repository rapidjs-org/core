{
  "name": "@rapidjs.org/core",
  "version": "3.3.0",
  "description": "rapidJS is a framework for developing lightweight asset focused server environments for scalable web applications. Furthermore it provides a simple, yet powerful interface for individualization of any complexity.",
  "main": "dist/app.js",
  "scripts": {
    "test-app": "node ./test-app/server.js",
    "test-app:dev": "node ./test-app/server.js -dev",
    "lint": "./node_modules/.bin/eslint --ext .ts --fix ./src/",
    "compile": "tsc",
    "test": "node ./scripts/test.js",
    "build": "npm run lint && rm -r ./dist && npm run compile && npm run test && node ./scripts/build.js",
    "release": "npm run build && np"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rapidjs",
    "rapidjs.org"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidjs-org/core.git"
  },
  "author": "Thassilo Martin Schiepanski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rapidjs-org/core/issues"
  },
  "homepage": "https://rapidjs.org",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.26.0",
    "eslint-plugin-typescript": "^0.14.0",
    "typescript": "^4.5.2",
    "uglify-js": "^3.14.3",
    "websocket": "^1.0.34"
  },
  "publishConfig": {
    "access": "public"
  }
}
