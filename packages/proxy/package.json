{
    "name": "@rapidjs.org/proxy",
    "version": "0.1.0",
    "description": "No- and low-code web application development framework for JavaScript",
    "author": "Thassilo Martin Schiepanski",
    "homepage": "https://rapidjs.org",
    "keywords": [
        "rapidjs",
        "rapidjs.org"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rapidjs-org/core.git"
    },
    "bugs": {
        "url": "https://github.com/rapidjs-org/core/issues"
    },
    "main": "./build/api.js",
    "types": "./types/api.d.ts",
    "files": [
        "./build/",
        "./types/"
    ],
    "scripts": {
        "debug": "npx tsc --project ./tsconfig.debug.json",
        "debug:watch": "npm run debug -- --watch",
        "build": "rm -rf ./build/ && npx tsc --project ./tsconfig.build.json",
        "test:unit": "npx rjs-test unit ./test/unit/",
        "test:http": "npx rjs-test http ./test/http/",
        "test": "nm run test:unit && npm run test:http"
    },
    "devDependencies": {
		"@rapidjs.org/test": "file:../app-test/packages/test",
		"@rapidjs.org/test--http": "file:../app-test/packages/@http",
		"@rapidjs.org/test--unit": "file:../app-test/packages/@unit",
        "@types/node": "^20.10.6",
        "typescript": "^5.3.3"
    }
}
